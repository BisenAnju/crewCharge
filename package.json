{
  "name": "create-react-app",
  "version": "3.2.2",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@ionic/react": "0.0.6-3",
    "axios": "^0.19.0",
    "cordova-android": "^7.1.4",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-browsertab": "^0.2.0",
    "cordova-plugin-buildinfo": "^2.0.2",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-customurlscheme": "^4.4.0",
    "cordova-plugin-inappbrowser": "^3.0.0",
    "cordova-plugin-whitelist": "1",
    "cordova-universal-links-plugin": "git+https://github.com/walteram/cordova-universal-links-plugin.git",
    "css-loader": "^3.0.0",
    "firebase": "^5.3.0",
    "i18n-js": "^3.3.0",
    "image-compressor.js": "^1.1.4",
    "material-ui": "^0.20.2",
    "material-ui-textfield-icon": "^0.2.7",
    "moment": "^2.24.0",
    "onesignal-cordova-plugin": "^2.4.7",
    "react": "^16.2.0",
    "react-big-calendar": "^0.21.0",
    "react-dom": "^16.8.6",
    "react-dropzone": "^9.0.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-swipeable-views": "^0.12.15"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && rm -rf www && mv build www",
    "buildW": "react-scripts build && ren build www",
    "buildB": "react-scripts build",
    "lint": "eslint src",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "yarn build && rm ./www/static/js/*.map && firebase deploy",
    "deployWindows": "yarn build && del www\\static\\js\\*.map && firebase deploy",
    "android": "yarn build && cordova run android",
    "ios": "yarn build && cordova run ios"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.3",
    "prettier": "1.15.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-universal-links-plugin": {},
      "cordova-plugin-buildinfo": {},
      "cordova-plugin-browsertab": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "com.smilebots.projects",
        "ANDROID_SCHEME": " ",
        "ANDROID_HOST": " ",
        "ANDROID_PATHPREFIX": "/"
      },
      "onesignal-cordova-plugin": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  }
}
